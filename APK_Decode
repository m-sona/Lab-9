def decode(password):    list(password)    originalpassword = []    for i in password:        if i == 7:            originalpassword.append(4)        elif i == 8:            originalpassword.append(5)        elif i == 9:            originalpassword.append(6)        elif i == 4:            originalpassword.append(1)        elif i == 5:            originalpassword.append(2)        elif i == 6:            originalpassword.append(3)        elif i == 3:            originalpassword.append(0)        elif i == 2:            originalpassword.append(9)        elif i == 1:            originalpassword.append(8)        elif i == 0:            originalpassword.append(7)    "".join(originalpassword)    return originalpassword